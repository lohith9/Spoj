#include <bits/stdc++.h>

using namespace std;

int main() {
   int t;
   cin>>t;
   while(t--){
       int n;
       cin>>n;
       int a[n],b[n];
       for(int i=0;i<n;i++){
           cin>>a[i];
       }
       sort(a,a+n);
       for(int i=0;i<n;i++){
           cin>>b[i];
       }
       sort(b,b+n);
       long long int sum=0;
       for(int i=0;i<n;i++){
           sum+=(a[i]*b[i]);
       }
       cout<<sum<<endl;
   }
}
